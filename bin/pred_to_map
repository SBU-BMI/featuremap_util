#!/usr/bin/env bash

NORMAL="\\033[0;39m"
RED="\\033[1;31m"
# Check input
if [[ "$#" -eq "0" ]]; then
  echo "No arguments supplied"
  printf "    ${RED}wsi type? svs, tif, or scn?${NORMAL}\n"
  exit 1
fi

ext="$1"
output='/data/output'

SECONDS=0

/app/src/prediction_to_map.sh "$ext" "$output"

ELAPSED="Elapsed: $(($SECONDS / 3600))hrs $(($SECONDS / 60 % 60))min $(($SECONDS % 60))sec"
echo "$ELAPSED"
