#!/usr/bin/env bash

PROGNAME=$(basename "$0")

if [ "$#" -ne 3 ]; then
  echo ""
  echo "usage: "
  echo "    ./$PROGNAME executionid executedby wsi_type"
  echo ""
  echo "    wsi_type: can be svs, tif, scn, etc"
  exit 1
fi

executionid="$1"
executedby="$2"
wsi_type="$3"
/app/src/prediction_to_map.sh "/data/input" "/data/output" "/data/wsi" "$wsi_type" "$executionid" "$executedby"
echo "$PROGNAME done."
