#!/usr/bin/env bash

PROGNAME=$(basename "$0")

if [ "$#" -ne 2 ]; then
  echo ""
  echo "usage: "
  echo "    ./$PROGNAME exec_id wsi_type"
  echo ""
  echo "    wsi_type: can be svs, tif, scn, etc"
  exit 1
fi

ext="$1"
/app/src/prediction_to_map.sh "/data/input" "/data/output" "/data/wsi" "$ext"
echo "$PROGNAME done."
