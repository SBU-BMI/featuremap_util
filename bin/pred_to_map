#!/usr/bin/env bash

PROGNAME=$(basename "$0")

if [ "$#" -ne 3 ]; then
  echo ""
  echo "usage: "
  echo "    ./$PROGNAME exec_id exec_by wsi_type"
  echo ""
  echo "    wsi_type: can be svs, tif, scn, etc"
  exit 1
fi

exec_id="$1"
exec_by="$2"
wsi_type="$3"
/app/src/prediction_to_map.sh "/data/input" "/data/output" "/data/wsi" "$wsi_type" "$exec_id" "$exec_by"
echo "$PROGNAME done."
